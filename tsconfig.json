{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "esnext",
    "module": "esnext",
    "declaration": false,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "outDir": "./build",
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "rootDir": ".",
    "importHelpers": true,
    "noEmitHelpers": true,
    "verbatimModuleSyntax": true
  }
}